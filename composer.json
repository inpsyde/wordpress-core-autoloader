{
  "name": "wp-proposal/core-autoloader",
  "require": {
    "php": ">=5.2.4"
  },
  "require-dev": {
    "php": ">=5.4.0",
    "brain/monkey": "~1.3",
    "inpsyde/monkery-test-case": "dev-master",
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.8|~5.1",
    "inpsyde/wp-tests-starter": "^1.0",
    "inpsyde/wordpress-dev": "~4.4.0"
  },
  "autoload-dev": {
    "classmap": [ "src/" ],
    "files": [ "src/functions.php" ]
  },
  "minimum-stability": "dev",
  "config": {
    "platform": {
      "php": "5.4.0"
    }
  }
}
